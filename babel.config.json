{
  "presets": ["@babel/preset-env"],
  "plugins": [
    ["babel-plugin-jsx-dom-expressions", {
      "moduleName": "./index",
      "delegateEvents": true,
      "wrapConditionals": true,
      "jsx": {
        "factory": "createElement",
        "factoryFragment": "createFragment"
      }
    }]
  ]
}
